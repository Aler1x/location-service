{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "y/osm-integration/(.*)": "<rootDir>/../libs/osm-integration/src/$1",
    "y/osm-integration": "<rootDir>/../libs/osm-integration/src"
  }
}